@import "mixins/media-query-variables"

section[data-section="faq"]
  color #000
  display flex
  justify-content center
  flex-flow column
  padding var(--section-vertical-padding) 12px

  .content
    width 64rem
    max-width 100%
    margin 0 auto

    a
      color inherit

  .logos
    text-align center
    & > svg
      max-width 100%
      margin 2rem 0
      height 4rem
